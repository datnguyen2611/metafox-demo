  "scripts": {
    "reset": " rimraf *.lock && rimraf node_modules/ && yarn bootstrap",
    "bootstrap": "metafox bootstrap",
    "test": "jest",
    "fix": "metafox fix",
    "start": "PORT=3000 metafox start --type=web",
    "start:admincp": "PORT=3001 metafox start --type=admincp",
    "start:installation": "PORT=3003 metafox start --profile=installation --type=installation",
    "start:dev0": "nodemon --watch scripts -e js scripts/server.js",
    "bundle": "yarn && yarn bootstrap && metafox build --production",
    "bundle:release": "yarn && yarn bootstrap && metafox build --profile=release --production",
    "build:installation": "NODE_ENV=production yarn metafox build:process --profile=installation --type=installation --production",
    "build:call": "NODE_ENV=production yarn metafox build:process --profile=call --type=call --production",
    "build:multi-target": "metafox build --profile=multi-target --production",
    "build:preview": "metafox build --profile=preview --production",
    "build:team": "metafox build --profile=team --production",
    "export": "metafox export",
    "fix:layout": "npx ts-node scripts/fix-layout.ts && yarn reload",
    "reload": "metafox reload",
    "reload:admincp": "metafox reload --profile=admincp --type=admincp",
    "reload:installation": "metafox reload --profile=installation --type=installation",
    "publish:installation": "metafox publish --type=installation --profile=installation --production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "jest junit",
    "outputDirectory": "./coverage",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
